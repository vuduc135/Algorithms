using System;
					
public class Program
{
	public static void Main()
	{
		var res = IsPrime(7);
		Console.WriteLine("Res:" + res);
	}
	
	static bool IsPrime(int n)
	{
		if (n == 0 || n == 1)
		{
			return false;
		}
		
		if ( n == 2 || n == 3)
		{
			return true;
		}
		
		var m = n/2;
		
		for (var i = 2; i <= m; i++)
		{
			if (n % i == 0)
			{
				return false;
			}
		}
		return true;
	}
}
